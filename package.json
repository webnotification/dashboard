{
  "name": "dashboard",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build:profile": "browserify -t [ babelify --presets [ react ] ] public/javascripts/profile.js -o public/javascripts/profile_final.js",
    "build:send_notification": "browserify -t [ babelify --presets [ react ] ] public/javascripts/send_notification.js -o public/javascripts/send_notification_final.js",
    "build:permission_analytics": "browserify -t [ babelify --presets [ react ] ] public/javascripts/permission_analytics.js -o public/javascripts/permission_analytics_final.js",
    "build:notification_analytics": "browserify -t [ babelify --presets [ react ] ] public/javascripts/notification_analytics.js -o public/javascripts/notification_analytics_final.js",
    "build": "npm run build:profile && npm run build:permission_analytics && npm run build:notification_analytics && npm run build:send_notification"
  },
  "dependencies": {
    "aws-sdk": "^2.2.26",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "~1.13.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "ejs": "^2.3.4",
    "express": "~4.13.1",
    "express-session": "^1.12.1",
    "griddle-react": "^0.3.1",
    "jade": "~1.11.0",
    "kerberos": "0.0.17",
    "material-ui": "^0.14.2",
    "mongoose": "^4.3.0",
    "morgan": "~1.6.1",
    "multer": "^1.1.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "react": "^0.14.6",
    "react-dom": "^0.14.3",
    "react-tap-event-plugin": "^0.2.1",
    "request": "^2.67.0",
    "serve-favicon": "~2.3.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {}
}
